page {
  background: #f0f0f0;
  font-family: "微软雅黑",
                 Helvetica,
                 Arial,
                 "Hiragino Sans GB",
                 "Source Han Sans CN",
                 "PingFang SC",
                 Roboto,
                 "Heiti SC",
                 "Microsoft Yahei",
                 sans-serif;
  background: #fff;
}

.swiper-tab {
  height: 20px;
  padding: 10px 0;
  background: #fff;
  display: flex;
  font-size: 17px;
  line-height: 20px;
  border-bottom: 1px solid #e6e6e6;
}

.swiper-tab-list {
  flex: 1;
  text-align: center;
  position: relative;
  color: #787878;
}

.swiper-tab-list:first-of-type {
  border-right: 1px solid #c8c8c8;
}

.swiper-tab-list.on {
  color: #fa4b22;
}

.swiper-tab-list.on::after {
  content: "";
  height: 2px;
  width: 100px;
  background: #fa4b22;
  position: absolute;
  left: 50%;
  margin-left: -50px;
  bottom: -10px;
}

.book-box, .order-box {
  width: 100%;
  position: relative;
  box-sizing: border-box;
}

.book-box {
  display: flex;
  margin-bottom: 51px;
}

.swiper-vertical-nav {
  width: 170rpx;
  box-sizing: border-box;
}

.swiper-vertical-box {
  box-sizing: border-box;
  flex: 1;
}

.order-box {
  background: #fff;
}

.swiper-vertical-tab {
  width: 100%;
  position: absolute;
  left: 0px;
  top: 0;
  min-height: 100%;
}

.swiper-vertical-tab view {
  font-size: 28rpx;
  color: #505050;
  box-sizing: border-box;
  padding: 20rpx 1em;
  border-bottom: 1px solid #eee;
  min-height: 120rpx;
  display: flex;
  align-items: center;
}

.swiper-vertical-tab {
  background: #f7f7f7;
}

.swiper-vertical-tab view.on {
  background: #fff;
  font-weight: bold;
  color: #333;
}

.swiper-vertical-box .vertical-list {
  height: 300px;
}

.swiper-vertical-box .vertical-list .title {
  height: 70rpx;
  margin-left: 20rpx;
  color: #000;
  font-size: 26rpx;
  font-weight: bold;
  border-bottom: 1px solid #eee;
  display: flex;
  align-items: center;
}

.type-detail {
  background: #fff;
  position: relative;
}

.type-detail>view {
  border-bottom: 1px solid #e5e5e5;
  margin-left: 10px;
  overflow: hidden;
  padding-left: 80px;
  box-sizing: border-box;
  position: relative;
  height: 92px;
  width: 100%;
}

.type-detail view image {
  width: 70px;
  height: 70px;
  border-radius: 4px;
  margin-top: 10px;
  margin-bottom: 10px;
  position: absolute;
  left: 0;
  top: 0;
}

.book-detail {
  width: 100%;
  position: relative;
  height: inherit;
}

.book-detail .food-title {
  font-size: 28rpx;
  color: #333;
  padding-top: 10px;
  display: block;
}

.book-detail .food-vol {
  font-size: 24rpx;
  color: #999;
  display: block;
}

.book-detail button, .book-result-detail button {
  float: left;
  width: 27px;
  height: 28px;
  border-radius: 14px;
}

.book-detail .price {
  position: absolute;
  bottom: 10px;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.book-detail>view text {
  float: left;
}

.book-detail>view view {
  float: left;
}

.book-detail>view {
  margin-top: 5px;
  width: 100%;
  overflow: hidden;
  position: relative;
  height: 28px;
}


.book-detail .money {
  font-size: 17px;
  color: #fa4b22;
  line-height: 27px;
}

.book-detail .operations {
  position: absolute;
  right: 20px;
  top: 0;
}

.operations text {
  line-height: 27px;
  color: #333;
  width: 35px;
  /* display: block; */
  text-align: center;
}

.Bill-layer {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 1;
}

.Bill {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 2;
}

.takeBill {
  height: 50px;
  background: #fff;
  position: relative;
  border-top: 1px solid #eee;
}

.takeBill .box-right {
  width: 120px;
  height: 52px;
  background: #fa4d22;
  position: absolute;
  right: 0;
  top: -1px;
  border-radius: 0;
  color: #fff;
  line-height: 52px;
  text-align: center;
}

.takeBill .box-right.notake {
  background: #eee;
  border: none;
  border-radius: none;
  font-size: 32rpx;
}

.box-left {
  margin-left: 30px;
}

.box-left>view {
  float: left;
}

.box-left .tips {
  width: 30px;
  position: relative;
  background: url(https://ae01.alicdn.com/kf/H57c36fbb981c42b18b69fbb02b7fa311Y.jpg) no-repeat left 12px;
  background-size: 100% auto;
  height: 40px;
  position: relative;
}

.box-left .tips view {
  position: absolute;
  right: -5px;
  top: 7px;
  width: 20px;
  height: 20px;
  background: #f55022;
  color: #fff;
  border-radius: 10px;
  text-align: center;
  line-height: 20px;
  font-size: 12px;
}

.box-left .moeny {
  color: #fa4b22;
  font-size: 17px;
  margin-left: 10px;
  margin-top: 15px;
}

.clearCart {
  height: 35px;
  text-align: right;
  background: #f5f5f5;
}

.clearCart button {
  background: none;
  float: right;
  color: #646464;
  line-height: 35px;
  font-size: 14px;
  padding-left: 16px;
  background: url(https://ae01.alicdn.com/kf/H46a91dd719a74b2fbe628fcee946dbc1B.jpg) no-repeat left center;
  background-size: 13px auto;
}

.clearCart button::after {
  border: 0 none;
}

.book-result-detail {
  overflow: scroll;
  max-height: 300px;
  background: #fff;
}

.book-result-detail>view {
  background: #fff;
  overflow: hidden;
  height: 50px;
  padding-top: 10px;
  padding-left: 15px;
  box-sizing: border-box;
  padding-right: 10px;
  border-bottom: 1px solid #e6e6e6;
}

.book-result-detail>view .menu_name {
  font-size: 28rpx;
  margin-top: 10rpx;
}

.book-result-detail>view text {
  float: left;
}

.book-result-detail>view view {
  float: right;
}

.food-detail {
  overflow: hidden;
  height: 36px;
  line-height: 36px;
}

.food-detail>text {
  float: left;
  color: #3c3c3c;
  font-size: 18px;
  display: block;
}

.food-detail>view {
  float: right;
}

.food-detail>view text {
  font-size: 15px;
  color: #999;
}

.place {
  height: 5px;
  width: 100%;
  background: #f0f0f0;
}

.order-list {
  border-top: 1px solid #e6e6e6;
  border-bottom: 1px solid #e6e6e6;
  margin-top: -1px;
  padding: 20px 15px;
}

.order-list>text {
  color: #3c3c3c;
  font-size: 17px;
  margin-bottom: 6px;
  display: block;
}

.order-list .time {
  color: #999;
  font-size: 13px;
  text-align: right;
}

.bdt {
  border-top: 1px solid #e6e6e6;
}

.hidden {
  display: none;
}

.show {
  display: block;
}

.weui-form-preview {
  background-color: rgb(255, 255, 255);
  border-top: 1px solid rgb(229, 229, 229);
  border-bottom: 1px none rgb(229, 229, 229);
  margin-bottom: 10px;
  border-bottom-style: solid;
}

.weui-form-preview__hd {
  padding: 10px 15px;
  border-bottom: 1px solid rgb(229, 229, 229);
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  align-content: flex-start;
  border-bottom-color: rgba(229, 229, 229, 1);
}

.weui-form-preview__label {
  color: rgb(153, 153, 153);
  font-size: 16px;
  padding: 0px;
  border-bottom: 1px none rgb(229, 229, 229);
  display: inline-block;
}

.wx-text_n5HRT0 {
  font-weight: normal;
  font-size: 24px;
}

.weui-form-preview__hd2 {
  padding: 10px 15px;
  border-bottom: 1px none rgb(229, 229, 229);
  display: block;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
  align-content: flex-start;
  border-bottom-style: solid;
}

.weui-form-preview__item {
  display: flex;
  flex: 1 1 0%;
  justify-content: space-between;
  align-items: flex-start;
  width: 100%;
  line-height: 1.5em;
}

.weui-form-preview__label2 {
  color: #999;
  font-size: 14px;
  padding: 0px;
  border-bottom: 1px none rgb(229, 229, 229);
  display: inline-block;
  min-width: 50px;
  text-align: justify;
  text-align-last: justify;
  width: 60px;
}

.weui-form-preview__value {
  color: rgb(153, 153, 153);
  font-size: 14px;
  max-width: 525rpx;
  text-align: right;
}

.weui-form-preview__btn_primary {
  border-style: none;
  border-radius: 0px;
  color: rgb(9, 187, 7);
  display: flex;
  justify-content: space-around;
  align-items: center;
  line-height: 50px;
  font-size: 16px;
}

.ddnunber {
  font-weight: normal !important;
  font-size: 28rpx;
  color: #666;
}

.page__bd {
  background: #eee;
}

.refresh {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 50px;
  border-top: 1px solid #eee;
  font-size: 18px;
}
